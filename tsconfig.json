{ 
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "storybook-static",
    "baseUrl": ".",
    "jsx": "react",
    "resolveJsonModule": true
  },
  "include": [
    "./packages/**/*"
  ],
  "rules": {
    "react/prop-types": ["off"]
  }
}
